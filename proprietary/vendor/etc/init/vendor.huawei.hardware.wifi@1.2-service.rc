service wifi_ext /vendor/bin/hw/vendor.huawei.hardware.wifi@1.2-service
    interface android.hardware.wifi@1.0::IWifi default
    interface vendor.huawei.hardware.wifi@1.1::IHwWifiExt default
    interface vendor.huawei.hardware.wifi@1.2::IHwWifiExt default
    class hal
    capabilities NET_ADMIN NET_RAW SYS_MODULE
    user wifi
    group wifi gps dhcp inet net_raw net_admin
    oneshot
    disabled
