            <ionoid01>0</ionoid01>
            <ionoid11>0</ionoid11>
            <earthorien>0</earthorien>
            <timemodellst>0</timemodellst>
            <dganss>0</dganss>
            <navmodel>0</navmodel>
            <addlnavmodel>0</addlnavmodel>
            <exteph>0</exteph>
            <rti>0</rti>
            <gnssdatabit>0</gnssdatabit>
            <almanac>0</almanac>
            <utcmodel>0</utcmodel>
            <addlutcmodel>0</addlutcmodel>
            <refmeasinfo>1</refmeasinfo>
            <auxinfo>1</auxinfo>
            <bdsdiffcorr>0</bdsdiffcorr>
            <bdsgridmodel>0</bdsgridmodel>
        </asstcontrol_msa>
        <asstcontrol_msb>
            <reftime>1</reftime>
            <ionospheremodel>0</ionospheremodel>
            <ionoid00>0</ionoid00>
            <!-- ionoid01 indicates that the parameters have been generated by BDS -->
            <ionoid01>0</ionoid01>
            <ionoid11>0</ionoid11>
            <earthorien>0</earthorien>
            <timemodellst>1</timemodellst>
            <dganss>0</dganss>
            <navmodel>1</navmodel>
            <addlnavmodel>0</addlnavmodel>
            <exteph>0</exteph>
            <rti>1</rti>
            <gnssdatabit>0</gnssdatabit>
            <almanac>1</almanac>
            <utcmodel>1</utcmodel>
